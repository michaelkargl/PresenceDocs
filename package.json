{
  "name": "zube",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "yarn": "yarn --prefer-offline --emoji true",
    "docusaurus": "docusaurus",
    "start": "yarn run yarn run docusaurus -- start",
    "build": "yarn run yarn run build:docusaurus",
    "swizzle": "yarn run yarn run docusaurus -- swizzle",
    "deploy": "yarn run yarn run docusaurus -- deploy",
    "serve": "yarn run yarn run docusaurus -- serve",
    "storybook": "storybook dev -p 6006",
    "build:all": "yarn run yarn run build:plugins && yarn run yarn run build && yarn run yarn run build:storybook",
    "build:docusaurus": "yarn run yarn run docusaurus -- build",
    "build:storybook": "storybook build",
    "build:plugins": "cd plugins/path-alias-plugin && npm run build && cd - && npm install"
  },
  "dependencies": {
    "@cortex-js/compute-engine": "^0.25.1",
    "@docusaurus/core": "^3.4.0",
    "@docusaurus/preset-classic": "^3.4.0",
    "@docusaurus/theme-mermaid": "^3.4.0",
    "@matejmazur/react-katex": "^3.1.3",
    "@mdx-js/react": "^3.0.0",
    "clsx": "^2.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rehype-katex": "^7.0.0",
    "remark-math": "^6.0.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.6.1",
    "@docusaurus/module-type-aliases": "^3.4.0",
    "@docusaurus/tsconfig": "^3.4.0",
    "@docusaurus/types": "^3.4.0",
    "@storybook/addon-essentials": "^8.2.1",
    "@storybook/addon-interactions": "^8.2.1",
    "@storybook/addon-links": "^8.2.1",
    "@storybook/addon-onboarding": "^8.2.1",
    "@storybook/addon-webpack5-compiler-swc": "^1.0.4",
    "@storybook/blocks": "^8.2.1",
    "@storybook/react": "^8.2.1",
    "@storybook/react-webpack5": "^8.2.1",
    "@storybook/test": "^8.2.1",
    "@types/react": "^18.2.29",
    "path-alias-plugin": "file:./libs/path-alias-plugin/dist",
    "storybook": "^8.2.1",
    "typescript": "^5.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=18.17"
  }
}
